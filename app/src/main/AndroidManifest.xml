<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jack.pramodshinde">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/bgimg"
        android:label="@string/app_name"
        android:roundIcon="@drawable/bgimg"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.GalleryActivity"></activity>
        <activity android:name=".activity.ContactActivity" />
        <activity android:name=".activity.AppointmentActivity" />
        <activity android:name=".activity.ComplaintActivity" />
        <activity android:name=".activity.JobActivity" />
        <activity android:name=".activity.WorkActivity" />
        <activity android:name=".activity.AboutActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>